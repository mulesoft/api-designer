{
  "name": "raml-summary-view",
  "version": "1.0.3",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "A basic information about the RAML definition viewer for the documentation panel",
  "devDependencies": {
    "polymer-cli": "^0.16.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "raml-summary-view.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint raml-summary-view.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"raml-summary-view:local-test\"",
    "test": "node_modules/.bin/polymer test",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/raml-summary-view.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/raml-summary-view/issues",
    "email": "arc@mulesoft.com"
  }
}